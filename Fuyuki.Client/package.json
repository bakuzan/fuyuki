{
  "name": "fuyuki",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ayaka": "file:../../ayaka/build",
    "classnames": "^2.2.6",
    "dashjs": "^3.1.3",
    "meiko": "file:../../meiko/build",
    "nano-css": "^5.2.1",
    "node-sass": "^4.13.0",
    "oidc-client": "^1.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.3",
    "ajv": "^6.9.1",
    "cross-env": "^5.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'"
  },
  "lint-staged": {
    "{,src}/**/*.{ts,tsx,js}": [
      "npm run lint",
      "git add"
    ],
    "{,src}/**/*.{tsx,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
